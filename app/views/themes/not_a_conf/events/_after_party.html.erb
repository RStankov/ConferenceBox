<section id="ts-after-party" class="ts-venue <%= background_class %>">
  <div class="container">
    <div class="row text-center">
      <span class="icon-wrap"><i class="fa fa-map"></i></span>
      <h2 class="section-title">After Party</h2>
      <div class="section-sub-title">
        <strong>
          <%= link_to current_event.after_party_venue_name, current_event.after_party_venue_site_url, target: '_blank' %>
          <%= current_event.after_party_venue_address if current_event.after_party_venue_address? %>
        </strong>
        <%= markdown current_event.after_party_venue_notes if current_event.after_party_venue_notes? %>
      </p>
    </div>

    <% if current_event.after_party_venue_map? %>
      <div class="row">
        <iframe width="100%" height="500" border="none" scrolling="no" src="<%= current_event.after_party_venue_map_embedded_url %>" class="map"></iframe>
        <div class="gap-60"></div>
      </div>
    <% end %>
  </div>
</section>
