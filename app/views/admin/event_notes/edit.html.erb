<% title 'Notes' %>

<% breadcrumbs ['Events', admin_events_path], [@event.conference.name, admin_conference_path(@event.conference)], [@event.name, admin_event_path(@event)] %>

<%= admin_form_for @event, url: admin_event_notes_path(@event) do |form| %>
  <%= form.error_notification %>
  <%= form.input :notes %>
  <%= form.buttons cancel_path: admin_event_path(@event) %>
<% end %>
